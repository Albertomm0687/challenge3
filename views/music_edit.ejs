
<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.id %>" method="POST">
          <div class="form-group">
            <input type="text" name="users" value="<%= data.users%>" placeholder="Users" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="albums" value="<%= data.albums%>" placeholder="Albums" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="country" value="<%= data.country%>" placeholder="Country" class="form-control">
          </div>

          <button type="submit" class="btn btn-primary">
            Edit music
          </button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>